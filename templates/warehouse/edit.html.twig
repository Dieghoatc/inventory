{% extends 'base.html.twig' %}

{% block title %}{{ 'warehouse.edit.title'|trans }}{% endblock %}

{% block body %}

    <div class="card">
        <div class="card-header">
            {{ 'warehouse.edit.title'|trans }}
        </div>
        <div class="card-body">
            <div class="form-group">
                <label for="{{ form.name.vars.id }}" >:)</label>
